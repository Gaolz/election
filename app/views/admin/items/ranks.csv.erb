<%=
    CSV.generate do |csv|
    csv << ["排名", "名称", "副标题" "得票数"]
        @items.each_with_index do |item, index|
            csv << [
                index.next.to_s.rjust(3, '0'),
                item.title,
                item.slug,
                item.score
            ]
        end
    end.html_safe
%>